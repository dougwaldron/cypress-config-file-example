{
  "name": "config-file-example",
  "version": "1.0.0",
  "description": "Sample cypress test suite using config files",
  "author": "Doug Waldron",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^4.11.0"
  },
  "scripts": {
    "verify": "cypress verify",
    "version": "cypress version",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "cypress:dev": "cypress open --env configFile=dev",
    "cypress:dev-run": "cypress run --env configFile=dev"
  }
}
